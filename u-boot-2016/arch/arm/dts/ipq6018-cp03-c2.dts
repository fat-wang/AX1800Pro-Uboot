/dts-v1/;
#include "ipq6018-soc.dtsi"
/ {
	model ="QCA, IPQ6018-CP03-C2";
	compatible = "qca,ipq6018", "qca,ipq6018-cp03-c2";
	machid = <0x8030201>;
	config_name = "config@cp03-c2";

	aliases {
		console = "/serial@78B1000";
		mmc = "/sdhci@7804000";
		usb0 = "/xhci@8a00000";
	};
	ess-switch {
		switch_mac_mode = <PORT_WRAPPER_PSGMII>;
		switch_mac_mode1 = <UNUSED>;
		malibu_gpio = <75>;
		malibu_gpio_cnt = <1>;
		mdc_mdio_gpio = <64 65>;
		port_phyinfo {
			port@0 {
				phy_address = <0>;
				phy_type = <MALIBU_PHY_TYPE>;
			};
			port@1 {
				phy_address = <1>;
				phy_type = <MALIBU_PHY_TYPE>;
			};
			port@2 {
				phy_address = <2>;
				phy_type = <MALIBU_PHY_TYPE>;
			};
			port@3 {
				phy_address = <3>;
				phy_type = <MALIBU_PHY_TYPE>;
			};
			port@4 {
				phy_address = <4>;
				phy_type = <MALIBU_PHY_TYPE>;
			};
		};
	};
	
	tlmm-gpio {

		led_gpio {

			gpio1 {
				gpio = <0x25>;
				func = <0x00>;
				out = <0x01>;
				pull = <0x01>;
				drvstr = <0x03>;
				oe = <0x01>;
				vm = <0x01>;
				od_en = <0x00>;
				pu_res = <0x02>;
			};
		};
	};
};

